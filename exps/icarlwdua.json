{
    "prefix": "reproduce",
    "dataset": "cifar100",
    "memory_size": 2000,
    "memory_per_class": 20,
    "fixed_memory": false,
    "shuffle": false,

    "init_cls": 10,
    "increment": 10,
    "model_name": "icarlwdua",
    "convnet_type": "resnet32",
    "device": ["0","1","2","3"],
    "seed": [2024],
    "EPSILON" : 1e-8,

    "init_epoch" :  50,
    "init_lr": 0.1,
    "init_milestones" : [60, 120, 170],
    "init_lr_decay" : 0.1,
    "init_weight_decay" : 0.0005,


    "epochs" :20,
    "lrate" : 0.1,
    "milestones" : [80, 120],
    "lrate_decay" : 0.1,
    "batch_size" : 128,
    "weight_decay" : 2e-4,
    "num_workers" : 8,
    "T" : 2
}

